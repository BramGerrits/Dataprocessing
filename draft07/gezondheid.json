    "ratings"
  ],
  "properties": {
    "ratings": {
      "type": "array",
      "uniqueItems": false,
      "minItems": 0,
      "additionalItems": true,



      "items": {
        "type": "object",


        "required": [
          "id",
          "gezondheid",
          "persoonskenmerken",
          "perioden",
          "ongelukkig",
          "gelukkig",
          "ontevreden",
          "tevreden"
        ],

            "properties": {
              "id": {
                "type": "integer"
              },
              "gezondheid": {
                "type": "string"
              },
              "persoonskenmerken": {
                "type": "string"
              },
              "perioden": {
                "type": "string"
              },
              "ongelukkig": {
                "type": [ "string", "null" ]
              },
              "gelukkig": {
                "type": [ "string", "null" ]
              },
              "ontevreden": {
                "type": [ "string", "null" ]
              },
              "tevreden": {
                "type": [ "string", "null" ]
              }
            }



      }



    }
  }
}